  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug unittests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bin/unittests/debug/unittests.elf",
      "args": [],
      //"stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "unittests",
    },
    {
      "name": "libMcu CortexM0plus tests, debug mode",
      "cwd": "${workspaceRoot}",
      "gdbPath": "/usr/bin/gdb-multiarch",
      "executable": "${workspaceFolder}/bin/test_LL_CortexM0plus/debug/test_LL_CortexM0plus.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "bmp",
      "interface": "swd",
      "device": "LPC812", // We use the LPC812 to test the cortexM0plus as we already have hardware for this
      "BMPGDBSerialPort": "/dev/ttyACM0",
      "powerOverBMP": "enable",
      "preLaunchCommands": [
        "set {int}0x40048000 = 2"
      ],
      "postLaunchCommands": [
        "set {int}0x40048000 = 2",
        "compare-sections"
      ],
      "preRestartCommands": [],
      "preLaunchTask": "build libMcu CortexM0plus tests, debug mode",
      "svdFile": "${workspaceFolder}/libraries/libMcu/svd/nxp/LPC812.svd",
      "debuggerArgs": [],
