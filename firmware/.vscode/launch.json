    "version": "0.2.0",
    "configurations": [
        {
            "name": "libMcuLL CortexM0plus tests, debug mode",
            "cwd": "${workspaceRoot}",
            "gdbPath": "/usr/bin/gdb-multiarch",
            "executable": "${workspaceFolder}/bin/test_LL_CortexM0plus/debug/test_LL_CortexM0plus.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "bmp",
            "interface": "swd",
            "device": "LPC812", // We use the LPC812 to test the cortexM0plus as we already have hardware for this
            "BMPGDBSerialPort": "/dev/ttyACM0",
            "powerOverBMP": "enable",
            "preLaunchCommands": [
                "set {int}0x40048000 = 2"
            ],
            "postLaunchCommands": [
                "set {int}0x40048000 = 2",
                "compare-sections"
            ],
            "preRestartCommands": [],
            "preLaunchTask": "build libMcuLL CortexM0plus tests, debug mode",
